{
  "env": {
    "browser": true,
    "commonjs": true,
    "node": true,
    "es6": true
  },
  "ecmaFeatures": {
    "jsx": true,
    "modules": true,
    "experimentalObjectRestSpread": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "linebreak-style": [
      1,
      "unix"
    ],
    "quotes": [
      1,
      "single"
    ],
    "comma-dangle": [
      1,
      "always-multiline"
    ],
    "no-console": [
      0
    ]
  },
  "plugins": [
    "react"
  ]
}
